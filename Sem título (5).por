programa {
  real numero1
  real numero2
  real pagar
  funcao inicio() {
    escreva("Por favor insira um numero")
    leia(numero1)
    escreva("Por favor insira outro numero")
    leia(numero2)
    escreva("O preço a se pagar é")
    pagar = (numero1 * numero2)
    escreva(pagar)
    
    
  }
}
